{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#000",
					"name": "Black",
					"slug": "custom-black"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "custom-white"
				},
				{
					"color": "#567271",
					"name": "Curator Green",
					"slug": "custom-curator-green"
				},
				{
					"color": "#a57979",
					"name": "Curator Rose",
					"slug": "custom-curator-rose"
				},
				{
					"color": "#39514f",
					"name": "Curator Green Hover",
					"slug": "custom-curator-green-hover"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-700-normal.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-500-normal.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-700-italic.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-500-italic.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-400-italic.woff2"
							]
						}
					],
					"fontFamily": "Montserrat, sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Lusitana",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lusitana/lusitana-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Lusitana",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lusitana/lusitana-700-normal.woff2"
							]
						}
					],
					"fontFamily": "Lusitana, serif",
					"name": "Lusitana",
					"slug": "lusitana"
				}
			],
			"fontSizes": []
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "10px"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--50)",
						"right": "var(--wp--preset--spacing--50)",
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/cover": {
				"css": ""
			},
			"core/navigation-submenu": {
				"css": ""
			}
		},
		"css": "",
		"elements": {
			"button": {
				"color": {
					"background": "#567271",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"letterSpacing": "1.5px",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "500",
					"letterSpacing": "1px",
					"textTransform": "uppercase"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)"
				}
			},
			"link": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--lusitana)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	]
}